<h1>You sent the following data in a {{reqType}} request:</h1>
<table>
	<tr>
		<th>Name</th>
		<th>Value</th>
		<th>Request Type</th>
	</tr>
  {{#each dataList}}
	<tr>
		<td id="{{this.reqType}}">{{this.name}}</td>
		<td id="{{this.reqType}}">{{this.value}}</td>
		<td id="{{this.reqType}}">{{this.reqType}}</td>
	</tr>
  {{/each}}
</table>